CHIP DMux8Way {
    IN in, sel[3];
    OUT a, b, c, d, e, f, g, h;

    PARTS:
    DMux(in=in, sel=sel[2], a=p, b=q);

    DMux(in=p, sel=sel[1], a=i, b=j);
    DMux(in=q, sel=sel[1], a=x, b=y);

    DMux(in=i, sel=sel[0], a=a, b=b);
    DMux(in=j, sel=sel[0], a=c, b=d);
    DMux(in=x, sel=sel[0], a=e, b=f);
    DMux(in=y, sel=sel[0], a=g, b=h);
}