CHIP FullAdder {
    IN a, b, c;  // 1-bit inputs
    OUT sum,     // Right bit of a + b + c
        carry;   // Left bit of a + b + c

    PARTS:
    HalfAdder(a=a, b=b, sum=tmpsum, carry=tmpcarry);
    HalfAdder(a=tmpsum, b=c, sum=sum, carry=tmp2carry);
    Or(a=tmpcarry, b=tmp2carry, out=carry);
}