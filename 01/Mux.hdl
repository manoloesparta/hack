CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:

    Not(in=a, out=nota);
    Not(in=b, out=notb);
    Not(in=sel, out=notsel);

    And(a=a, b=b, out=ab);

    And(a=notsel, b=a, out=notsa);
    And(a=notsa, b=notb, out=notsanotb);

    And(a=sel, b=nota, out=snota);
    And(a=snota, b=b, out=snotab);

    Or(a=ab, b=notsanotb, out=primer);
    Or(a=primer, b=snotab, out=out);
}